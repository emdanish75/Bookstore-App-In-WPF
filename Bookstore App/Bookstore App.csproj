<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <RootNamespace>Bookstore_App</RootNamespace>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="cup.png" />
    <None Remove="espresso.jpg" />
    <None Remove="flash.jpeg" />
    <None Remove="world.jpg" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="cup.png" />
    <Resource Include="espresso.jpg" />
    <Resource Include="flash.jpeg" />
    <Resource Include="world.jpg" />
  </ItemGroup>

</Project>
